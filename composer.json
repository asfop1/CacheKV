{
    "name": "asfop/cache-kv",
    "description": "A flexible, high-performance key-value caching library for PHP, supporting automatic fetching, batch operations, tag-based invalidation, and sliding expiration.",
    "license": "MIT",
    "keywords": [
        "php",
        "cache",
        "data",
        "batch"
    ],
    "authors": [
        {
            "name": "xxx",
            "email": "xxx@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Asfop\\CacheKV\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asfop\\CacheKV\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.5 || ^8.5 || ^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    }
}