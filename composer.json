{
    "name": "asfop/query-cache",
    "description": "A flexible, ORM-agnostic caching layer for database queries, supporting single and batch data retrieval.",
    "license": "MIT",
    "keywords": [
        "php",
        "cache",
        "query",
        "database",
        "data",
        "batch"
    ],
    "authors": [
        {
            "name": "xxx",
            "email": "xxx@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Asfop\\QueryCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asfop\\QueryCache\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.5 || ^8.5 || ^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    }
}